{
  "name": "react-typescript-webpack-starter",
  "version": "1.0.0",
  "description": "A starter template for React with Typescript using Webpack 5",
  "main": "build/server.js",
  "scripts": {
    "tsnode": "tsnd --respawn server.ts",
    "tsdev": "concurrently \"tsnd --project tsnodeconfig.json --respawn server.ts --watch backend --transpile-only\" \"webpack serve --config webpack.config.ts\"",
    "tsdevm": "concurrently \"tsnd --respawn server.ts --watch backend --transpile-only\" \"webpack-dev-server\" \"npm run test\" ",
    "test": "mocha --require ts-node/register --watch-extensions ts,tsx --require @babel/register --require ./tests/helpers.js --require ./tests/dom.js --require ignore-styles ./frontend/**/*test.js  -w",
    "dev": "concurrently \"nodemon server.js\" \"webpack-dev-server\" \"npm run test\" ",
    "tscf": "tsc --project tsconfig.json",
    "tscb": "tsc --project tsnodeconfig.json",
    "startbuild": "node build/server.js",
    "kill": "concurrently \"kill-port 8080\" \"kill-port 3000\"",
    "start": "npm run buildprod && npm run startbuild",
    "buildprod": "npm run clean && npm run build && npm run tscb",
    "build": "webpack --config webpack.config.ts",
    "clean": "rimraf dist && rimraf build"
  },
  "keywords": [
    "React",
    "TypeScript",
    "Webpack 5",
    "Starter Template",
    "Boilerplate"
  ],
  "author": "Vishwas",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.20.5",
    "@babel/register": "^7.18.9",
    "@types/uuid": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "colors": "^1.4.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.8",
    "eslint-loader": "^4.0.2",
    "express": "^4.18.2",
    "flux": "^4.0.3",
    "gibberish-aes": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "kill-port": "1.6.0",
    "mini-css-extract-plugin": "^2.7.2",
    "mongoose": "^5.13.15",
    "morgan": "^1.10.0",
    "node-sass": "^8.0.0",
    "postcss-scss": "^4.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass-loader": "^13.2.0",
    "source-map-loader": "^4.0.1",
    "stack-trace": "^1.0.0-pre1",
    "stylelint-scss": "^4.3.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "try-require": "^1.2.1",
    "uniqid": "^5.4.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@babel/runtime": "^7.13.10",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.7",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.1.0",
    "husky": "4",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "ts-loader": "6.2.1",
    "ts-mocha": "8.0.0",
    "ts-node": "10.2.0",
    "ts-node-dev": "1.1.8",
    "typescript": "^4.1.5",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
